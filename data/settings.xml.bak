<?xml version="1.1" ?>
<settings>
    <global>
        <autocomplete>
            <visible>true</visible>
        </autocomplete>
        <battle>
            <melee_distance>2</melee_distance>
            <range_distance>21</range_distance>
            <targets>Apex Bats,Apex Crab,Apex Jagil,Apex Knight Lugcrawler,Apex Rumble Lugcrawler,Apex Toad,Locus Armet Beetle,Locus Colibri,Locus Dire Bat,Locus Ghost Crab</targets>
            <trusts>Kupipi,Joachim,Koru-Moru,Qultada,Kupofried</trusts>
        </battle>
        <chat>
            <ipc_enabled>true</ipc_enabled>
        </chat>
        <discord>
            <channels>
                <support>https://discord.com/channels/1069136494616399883/1242295505334173716</support>
            </channels>
            <url>https://discord.gg/CfPxDy759J</url>
        </discord>
        <donate>
            <url>https://www.buymeacoffee.com/cyrite</url>
        </donate>
        <flags>
            <check_files>true</check_files>
            <show_death_warning>true</show_death_warning>
        </flags>
        <follow>
            <auto_pause>false</auto_pause>
            <distance>1</distance>
        </follow>
        <gearswap>

        </gearswap>
        <help>
            <mode_text_enabled>true</mode_text_enabled>
            <wiki_base_url>https://github.com/cyritegamestudios/trust/wiki</wiki_base_url>
        </help>
        <locales>
            <actions>

            </actions>
            <default />
            <font_names>
                <english>Arial</english>
                <japanese>MS Gothic</japanese>
            </font_names>
        </locales>
        <logging>
            <enabled>false</enabled>
            <filter_pattern />
            <logtofile>false</logtofile>
        </logging>
        <menu_key>%^numpad+</menu_key>
        <remote_commands>
            <whitelist />
        </remote_commands>
        <shortcuts>
            <menus>
                <gambits>
                    <enabled>false</enabled>
                    <flags>1</flags>
                    <key>G</key>
                </gambits>
                <modes>
                    <enabled>false</enabled>
                    <flags>1</flags>
                    <key>M</key>
                </modes>
                <skillchains>
                    <enabled>false</enabled>
                    <flags>1</flags>
                    <key>S</key>
                </skillchains>
            </menus>
        </shortcuts>
        <sounds>
            <sound_effects>
                <disabled>false</disabled>
            </sound_effects>
        </sounds>
        <updater>
            <manifest_url>https://raw.githubusercontent.com/cyritegamestudios/trust/main/manifest.json</manifest_url>
        </updater>
        <verbose>true</verbose>
    </global>
